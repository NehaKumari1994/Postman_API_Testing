									"    //verify the first name == Jim\r",
									"    var responseJSON = pm.response.json()\r",
									"        var booking = responseJSON.booking\r",
									"        var firstname = booking.firstname\r",
									"\r",
									"\r",
									"           pm.expect(firstname).to.be.equal(\"Jim\")\r",
									"})\r",
